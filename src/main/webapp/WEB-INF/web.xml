<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>addempl</servlet-name>
    <servlet-class>Servlets.AddEmployeeServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>loginempl</servlet-name>
    <servlet-class>Servlets.LoginEmployeeServlet</servlet-class>
  </servlet>

  <!-- *********************************************************************** -->

  <servlet-mapping>
    <servlet-name>loginempl</servlet-name>
    <url-pattern>/loginempl</url-pattern>
  </servlet-mapping>
</web-app>
